# .bash_profile

# Get the aliases and functions
if [ -f ~/.bashrc ]; then
	. ~/.bashrc
fi

# User specific environment and startup programs
alias webstorm="/opt/webstorm/bin/webstorm.sh"
alias pycharm="/opt/webstorm/bin/webstorm.sh"
alias rubymine="/opt/rubymine/bin/rubymine.sh"

PATH=$PATH:$HOME/.local/bin:$HOME/bing

export PATH="$HOME/.rbenv/bin:$PATH"

eval "$(rbenv init -)"

export ANDROID_HOME=/opt/android-sdk/platforms/android-23
export PATH=${PATH}:/opt/android-sdk/platform-tools:/opt/android-sdk/tools
